@{
    ViewBag.Title = "Home Page";
}

    <div class="row">
        <form class="well col" action="~/Home/DoLogin" method="post" class="well col-lg-8 col-lg-offset-2" style="margin-top:60px;">

            <label>Username</label>

            <input type="text" name="username" value="" class="form-control" />

            <label>Password</label>
            <input type="password" name="password" value="" class="form-control" />

            <input type="submit" name="" value="Log In" class="btn btn-primary" />
        </form>
    </div>

    <div class="row">
        <form action="~/Home/DoFeedback" method="post" class="well col-lg-8 col-lg-ofset-2" style="margin-top:60px;">

            <label>Feedback</label>
            <textarea name="feedback" class="form-control"></textarea>

            <label>Password</label>

            <input type="submit" name="" value="Log In" class="btn btn-primary" />
        </form>
    </div>
